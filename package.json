{
  "name": "spinoza-ethics-logic",
  "version": "0.1.0",
  "description": "A data-driven SPA for exploring Spinoza's Ethics with formal logic annotations.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.node.json && vite build && cp docs/index.html docs/404.html",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "echo 'Linting not configured'",
    "fetch:raw": "node --loader ts-node/esm scripts/fetch-raw-ethics.ts",
    "build:corpus": "node --loader ts-node/esm scripts/build-ethics-corpus.ts",
    "build:badges": "node scripts/build-badges.mjs",
    "badges": "node scripts/update-badges.mjs",
    "release": "node scripts/release.mjs",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "framer-motion": "^12.23.24",
    "katex": "^0.16.25",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-katex": "^3.1.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
